{
	"name": "website",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"dev": "vite --host",
		"prebuild": "rimraf dist",
		"build": "vue-tsc && vite build --mode production",
		"preview": "vite preview",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
		"format": "prettier --write src/",
		"gen:fonts": "node ./extras/fonts/script/ttf2woff2.cjs",
		"gen:spiderFonts": "font-spider ./extras/fonts/script/*.html"
	},
	"dependencies": {
		"@mdi/js": "^7.2.96",
		"@vueuse/core": "^10.9.0",
		"@vueuse/router": "^10.9.0",
		"aplayer": "^1.10.1",
		"axios": "^1.4.0",
		"canvas-confetti": "^1.9.2",
		"cropperjs": "^2.0.0-beta.3",
		"dayjs": "^1.11.10",
		"galmuri": "^2.38.7",
		"gsap": "^3.12.5",
		"mousesnow": "^0.0.3",
		"pinia": "^2.0.36",
		"pinyin-pro": "^3.19.6",
		"roughjs": "^4.6.6",
		"three": "^0.158.0",
		"vue": "^3.4.21",
		"vue-i18n": "^9.13.1",
		"vue-router": "^4.3.0",
		"xtt-ui": "0.2.1-alpha.1",
		"xtt-utils": "^0.2.2-alpha.0"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.2.0",
		"@types/node": "^20.4.5",
		"@types/three": "^0.158.1",
		"@types/vue": "^2.0.0",
		"@vitejs/plugin-vue": "^4.2.1",
		"@vue/eslint-config-prettier": "^7.1.0",
		"eslint": "^8.40.0",
		"eslint-plugin-vue": "^9.11.1",
		"font-spider": "^1.3.5",
		"prettier": "^2.8.8",
		"rimraf": "^5.0.1",
		"ttf2woff2": "^5.0.0",
		"typescript": "^5.1.6",
		"vite": "^4.3.5",
		"vue-tsc": "^1.8.8"
	}
}
